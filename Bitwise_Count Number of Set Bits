#include <stdio.h>

int main() {
    int n, count=0;
    scanf("%d", &n);
    if(n&1){
        count++;
          }
     if(1&(n>>1)){
        count++;
          }
    if(1&(n>>2)){
        count++;
          } 
    if(1&(n>>3)){
        count++;
          }
    printf("%d", count);
    return 0;
}
