#include <stdio.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    signed int n;
    scanf("%d", &n);
    if(n == 0){
        printf("No\n");
     }
    else if((n & (n + 1)) == 0){
        printf("Yes");
    }
    else{
        printf("No");
    }
    return 0;
}
