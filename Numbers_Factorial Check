#include <stdio.h>

int main() {
    int N, x = 1, factorial = 1;
    scanf("%d", &N);
    
    while(factorial < N){
        x++;
        factorial *= x;
    }
        if(N == factorial){
            printf("%d", x);
        }
           else{
               printf("NO");
           }
    
    return 0;
}
