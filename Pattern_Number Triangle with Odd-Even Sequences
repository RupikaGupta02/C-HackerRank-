#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    
    int start = 1;
    for(int i=1; i<=n; i++){
        int num =  start;
        
        for(int j=1; j<=i; j++){
            printf("%d ", num);
            num += 2;
        }
        printf("\n");
        
        if( start == 1)
            start = 2;
        else 
            start = 1;
        
    }
    return 0;
}
