int main() {
   int n;
    scanf("%d", &n);
    
    int arr[n];
    for(int i = 0; i < n ;i++){
        scanf("%d", &arr[i]);
        
    }
    int target, count = 0;
    scanf("%d", & target);
    
    for(int i = 0 ; i<= n ; i++){
        if(arr[i] == target)
            count++;
        
    }
    printf("%d", count);

    return 0;
}
